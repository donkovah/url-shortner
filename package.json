    "express": "4.17.1",
    "express-validator": "6.9.2",
    "faker": "^6.6.6",
    "http-errors": "^2.0.0",
    "http-status": "^1.5.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "inversify-logger-middleware": "^3.1.0",
    "jest": "^28.1.3",
    "joi": "^17.6.0",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.7.1",
