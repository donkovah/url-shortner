    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "eslint-plugin-security": "^1.5.0",
    "nodemon": "^2.0.7",
    "ts-node": "9.1.1"
